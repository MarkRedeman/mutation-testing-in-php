function sum_is_zero(array $values)
{
    $sum = 0;

    foreach ($values as $value) {
        $sum += $value;
    }

    return $sum === 0;
}

function sum_is_zero(array $values)
{
    $sum = 0;

    foreach ($values as $value) {
        $sum -= $value; // mutated "+" to "-"
    }

    return $sum === 0;
}